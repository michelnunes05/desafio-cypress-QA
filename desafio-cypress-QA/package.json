{
  "name": "desafio-cypress-qa",
  "version": "1.0.0",
  "description": "Automação de testes E2E com Cypress - Carrinho de Compras",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "test": "cypress run",
    "test:carrinho": "cypress run --spec 'cypress/e2e/carrinho/**'",
    "test:login": "cypress run --spec 'cypress/e2e/login/**'",
    "test:headed": "cypress run --headed"
  },
  "keywords": ["cypress", "e2e", "testing", "automation", "ecommerce"],
  "author": "Seu Nome",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.6.0"
  }
}